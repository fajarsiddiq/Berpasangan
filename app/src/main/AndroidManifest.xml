<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fajarsiddiq.berpasangan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.orm.SugarApp"
        android:theme="@style/AppTheme">

        <activity
            android:name=".module.main.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".module.board.BoardActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"/>

        <activity
            android:name=".module.result.ResultActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"/>

        <meta-data android:name="DATABASE" android:value="berpasangan.db" />
        <meta-data android:name="VERSION" android:value="1" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.fajarsiddiq" />

    </application>

</manifest>
